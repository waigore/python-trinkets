let counter = 0;

while (counter < 10) {
  counter = counter + 1;
  if (counter > 5) {
    continue;
  }
  print(counter);

}

print("Counter after 1st while:", counter);

while (counter < 20) {
  counter = counter + 1;
  if (counter == 15) {
    print("Counter ==", counter, ", exiting loop!");
    break;
  }
}

print("Counter after 2nd while:", counter);
